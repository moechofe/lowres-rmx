BG SOURCE ROM(3)

SPRITE 0,160,60,1
SPRITE 0 SIZE 1 PAL 0

E=0
NEXTE=0
M=0
X=0.5

DO
  CLS
  PAL 2
  BG 1
  BG COPY 0,0,13,3 TO 4,10
  BG COPY 0,0,13,3 TO 4,13
  BG 0
  PAL 0
  BG FILL 0,34 TO 32,34 CHAR 16
  PAL 1
  TEXT 5,8,"FX=EASE(E,M,X)"

  TEXT 5,11,"E="
  NUMBER 7,11,E,1
  RESTORE EASING
  SKIP E
  READ T$
  TEXT 9,11,T$
  
  RESTORE MODE
  SKIP M+1
  READ T$
  TEXT 5,14,"M="+STR$(M)+" "+T$

  TEXT 5,17,"X="+LEFT$(STR$(X),4)

  FX=EASE(E,M,X)

  TEXT 5,19,"FX="+LEFT$(STR$(FX),4)
  
  Y=FX*196
  SPRITE 0,,Y+60,

  WAIT VBL

  TX=TOUCH.x\8
  TY=TOUCH.y\8
  IF TAP AND TX>=5 and TY=11 AND TX<=15 THEN e=(E+1) Mod 10
  IF TAP AND TX>=5 and TY=14 AND TX<=15 THEN m=((m+2) mod 3)-1
  IF TAP THEN X=0
  
  X=MIN(1,X+1/60)

LOOP

EASING:
DATA "LINEAR", "SINE", "QUAD", "CUBIC", "QUART", "QUINT", "CIRC", "BACK", "ELASTIC", "BOUNCE"

MODE:
DATA "IN","INOUT","OUT"
        


